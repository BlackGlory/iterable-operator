{
  "name": "iterable-operator",
  "version": "2.2.0",
  "description": "Utilities for JavaScript Iterable and AsyncIterable",
  "keywords": [
    "Iterable",
    "AsyncIterable",
    "chunk",
    "concat",
    "drop",
    "filter",
    "flatten",
    "map",
    "repeat",
    "slice",
    "split",
    "take",
    "tap",
    "transform",
    "uniq",
    "zip",
    "consume",
    "each",
    "every",
    "find",
    "first",
    "includes",
    "last",
    "match",
    "reduce",
    "some"
  ],
  "files": ["lib"],
  "main": "lib/es2018/index.js",
  "types": "lib/es2018/index.d.ts",
  "sideEffects": false,
  "repository": "git@github.com:BlackGlory/iterable-operator.git",
  "author": "BlackGlory <woshenmedoubuzhidao@blackglory.me>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --quiet src __tests__",
    "test": "jest --runInBand --config jest.config.js",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:coverage": "jest --coverage --config jest.config.js",
    "prepublishOnly": "run-s clean build",
    "clean": "rimraf lib",
    "build": "run-p build:*",
    "build:es2015": "run-s build:es2015:*",
    "build:es2015:compile": "tsc --project tsconfig.build.json --module commonjs --target es2015 --outDir lib/es2015",
    "build:es2015:patch": "tscpaths -p tsconfig.build.json -s ./src -o ./lib/es2015",
    "build:es2018": "run-s build:es2018:*",
    "build:es2018:compile": "tsc --project tsconfig.build.json --module commonjs --target es2018 --outDir lib/es2018",
    "build:es2018:patch": "tscpaths -p tsconfig.build.json -s ./src -o ./lib/es2018",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s lint build test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@blackglory/jest-matchers": "^0.5.0",
    "@blackglory/pass": "^1.1.0",
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "eslint": "^8.27.0",
    "husky": "^4.3.8",
    "jest": "^29.3.1",
    "jest-extended": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "return-style": "^1.0.0",
    "rimraf": "^3.0.2",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.0.3",
    "tscpaths": "^0.0.9",
    "tslib": "^2.4.1",
    "typescript": "4.8.4"
  },
  "dependencies": {
    "@blackglory/errors": "^2.3.0",
    "@blackglory/go": "^1.1.2",
    "@blackglory/types": "^1.4.0",
    "justypes": "^3.0.0"
  }
}
