{
  "name": "iterable-operator",
  "version": "4.0.7",
  "description": "Utilities for JavaScript Iterable and AsyncIterable",
  "keywords": [
    "Iterable",
    "AsyncIterable",
    "chunk",
    "concat",
    "drop",
    "filter",
    "flatten",
    "map",
    "repeat",
    "slice",
    "split",
    "take",
    "tap",
    "transform",
    "uniq",
    "zip",
    "consume",
    "each",
    "every",
    "find",
    "first",
    "includes",
    "last",
    "match",
    "reduce",
    "some"
  ],
  "files": [
    "src",
    "lib"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "type": "module",
  "sideEffects": false,
  "repository": "git@github.com:BlackGlory/iterable-operator.git",
  "author": "BlackGlory <woshenmedoubuzhidao@blackglory.me>",
  "license": "MIT",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "prepare": "ts-patch install -s",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --quiet src __tests__",
    "test": "cross-env NODE_OPTIONS='--experimental-vm-modules --max-old-space-size=4096' jest --runInBand --config jest.config.cjs",
    "test:debug": "cross-env NODE_OPTIONS=--experimental-vm-modules node --inspect-brk node_modules/.bin/jest --runInBand --config jest.config.cjs",
    "test:coverage": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --coverage --config jest.config.cjs",
    "prepublishOnly": "run-s prepare clean build",
    "clean": "rimraf lib",
    "build": "tsc --project tsconfig.build.json --outDir lib",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s prepare lint build test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@blackglory/jest-resolver": "^0.3.0",
    "@blackglory/pass": "^1.1.0",
    "@commitlint/cli": "^17.5.1",
    "@commitlint/config-conventional": "^17.4.4",
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.37.0",
    "extra-sort": "^0.1.1",
    "husky": "^4.3.8",
    "jest": "^29.5.0",
    "jest-resolve": "^29.5.0",
    "npm-run-all": "^4.1.5",
    "return-style": "^3.0.0",
    "rimraf": "^4.4.1",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.1.0",
    "ts-patch": "^2.1.0",
    "tslib": "^2.5.0",
    "typescript": "4.8.4",
    "typescript-transform-paths": "^3.4.6"
  },
  "dependencies": {
    "@blackglory/errors": "^3.0.2",
    "@blackglory/go": "^1.1.2",
    "@blackglory/structures": "^0.13.3",
    "extra-abort": "^0.3.6",
    "extra-promise": "^6.0.6",
    "extra-utils": "^5.1.1",
    "justypes": "^4.2.0"
  }
}
