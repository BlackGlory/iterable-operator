{
  "name": "iterable-operator",
  "version": "5.0.0",
  "description": "Utilities for JavaScript Iterable and AsyncIterable",
  "keywords": [
    "Iterable",
    "AsyncIterable",
    "chunk",
    "concat",
    "drop",
    "filter",
    "flatten",
    "map",
    "repeat",
    "slice",
    "split",
    "take",
    "tap",
    "transform",
    "uniq",
    "zip",
    "consume",
    "each",
    "every",
    "find",
    "first",
    "includes",
    "last",
    "match",
    "reduce",
    "some"
  ],
  "files": [
    "src",
    "lib"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "type": "module",
  "sideEffects": false,
  "repository": "git@github.com:BlackGlory/iterable-operator.git",
  "author": "BlackGlory <woshenmedoubuzhidao@blackglory.me>",
  "license": "MIT",
  "engines": {
    "node": ">=18.17.0"
  },
  "scripts": {
    "prepare": "ts-patch install -s",
    "lint": "eslint --quiet src __tests__",
    "test": "vitest --run",
    "prepublishOnly": "run-s prepare clean build",
    "clean": "rimraf lib",
    "build": "tsc --project tsconfig.build.json --outDir lib",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s prepare lint build test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@blackglory/pass": "^1.1.1",
    "@commitlint/cli": "^19.2.2",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/js": "^9.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.1.0",
    "extra-sort": "^0.1.2",
    "husky": "^4.3.8",
    "npm-run-all": "^4.1.5",
    "return-style": "^3.0.1",
    "rimraf": "^5.0.5",
    "standard-version": "^9.5.0",
    "ts-patch": "^3.1.2",
    "tslib": "^2.6.2",
    "typescript": "5.4.5",
    "typescript-eslint": "^7.7.0",
    "typescript-transform-paths": "^3.4.6",
    "vite": "^5.2.9",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.5.0"
  },
  "dependencies": {
    "@blackglory/errors": "^3.0.3",
    "@blackglory/go": "^2.0.0",
    "@blackglory/structures": "^0.14.2",
    "extra-abort": "^0.3.9",
    "extra-promise": "^6.2.0",
    "extra-utils": "^5.6.0",
    "justypes": "^4.2.1"
  }
}
